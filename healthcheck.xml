<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Report" parallel="classes" thread-count="5">

	<listeners>
		<listener class-name="Extentreports.Extentreportdemo.ListenerReporterForHealthcheck"></listener>
	</listeners>

	<parameter name="OS" value="Windows" />
	<parameter name="browser" value="Chrome" />

	<test name="Healthcheck Report">
		<classes>
			<class name="Extentreports.Extentreportdemo.Healthcheck"> </class>
		</classes>
	</test>
</suite> <!-- Suite -->
